import java.io.File;
import java.io.FileNotFoundException;
import java.util.*;

public class MapReader {
	public int vertCount;
	
	public int countVert(File f) throws FileNotFoundException{
		int count = 0;
		Scanner s = new Scanner(f);
		return count;
	}
}

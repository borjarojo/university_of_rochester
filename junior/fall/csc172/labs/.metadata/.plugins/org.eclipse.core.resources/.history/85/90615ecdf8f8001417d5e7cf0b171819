	*/
	
	public static <T extends Comparable<? super T>> Comparable<T> getMax(Comparable<T>[] array){
		Comparable<T> thing = array[0];
		
		for(int i = 0; i < array.length; i++){
			if(array[i].compareTo(thing) == 1)
				thing = array[i];
		}
		
		return thing;
	}
	
}
